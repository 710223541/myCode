/* CSS Document */
body{margin:0; padding:0; font-size:12px; font-family:"微软雅黑"; font-size: 0.875em; line-height:28px; color:#333; background:#f2f2f2;}
h1,h2,h3,h4,h5,h6,p,ol,li,ul,li,dl,dt,dd,input,select{margin:0; padding:0; list-style:none;}
input,a{outline:none;}
img{border:none;}
a:link,a:visited{color:#fff; text-decoration:none;}
a:hover{color:#fff;}

.fl{float:left;}
.fr{float:right;}
.mt20{margin-top:1.25px;}
.mt50{margin-top:3.125px;}
.mb20{margin-bottom:1.25px;}
.mr2{margin-right:2%;}
.mb20{margin-bottom:20px;}
.mb60{margin-bottom:60px;}

.ft16{font-size:16px;}
.ft18{font-size:18px;}
.ft20{font-size:20px;}
.ft22{font-size:22px;}
.corF{color:#fff;}
.cor80{color:#808080;}
.cor333{color:#333;}
.fwN{font-weight:normal;}


/* 首页 */
.ly-header{width:100%; padding:29px 0 15px 0;  height:100px; background:url(../images/index_head.png) no-repeat; background-size:100% 144px;
	/*background:-webkit-gradient(linear, 0 0, 0 100%, from(#1F69B4), to(#77A7E9));
	background:-webkit-linear-gradient(top, #1F69B4, #77A7E9);
	background:-o-linear-gradient(top, #1F69B4, #77A7E9);
	background:-o-linear-gradient(to bottom, #1F69B4, #77A7E9);
	background:-moz-linear-gradient(top, #1F69B4, #77A7E9);
	background:-moz-linear-gradient(to bottom, #1F69B4, #77A7E9);*/
}
.ly-header-wrap{padding:0 30px;}
.ly-header h2{text-align:center;}

.txt_wrap{border-radius:2em; background:#fff;display:-webkit-box; display:-moz-box; display:box; width:100%; margin-top:30px;}
.input_txt{position:relative; -webkit-box-flex:1; -moz-box-flex:1; box-flex:1;}
.ly-input-txt-wrap{padding-left:32px; border-radius:2em;}
.input_txt-span1{position:absolute; left:10px; top:6px;}
.search_txt{position:relative; border:0; height:32px; line-height:32px; width:100%; border-radius:2em;}
.ly-article{margin-top:24px; padding:0 30px; overflow:hidden; background:#fff;}
.ly-article ul{width:100%; font-size:20px; margin:0 auto;}
.ly-article ul li{width:49%; float:left; height:90px; line-height:90px; text-align:center; border-radius:8px; margin-bottom:2%;}
.ly-article ul li a{display:inline-block; width:100%;}
.ly-article-li1{background:#ADED51;}
.ly-article-li2{background:#3FD9D1;}
.ly-article-li3{background:#3397F1;}
.ly-article-li4{background:#5561E7;}
.ly-article-li5{background:#de5f93;}

/* 底部 */
.ly-footer{width:100%; background-color:#F6F6F6; border-top:1px solid #D3D3D3; position:fixed; bottom:0; z-index:9999;}
.ly-footer ul{width:100%; font-size:14px;}
.ly-footer li{width:50%; float:left; height:60px; text-align:center; line-height:20px;}
.ly-footer li a{color:#333; text-decoration:none;}
.ly-footer .cur{color:#4D8DD1;}
.ly-footer ul span{display:block; margin-top:8px;}


/* 搜索页 */
.head{width:100%; height:48px; background:#4D8DD1; line-height:48px;}
.head-top{padding:0 30px;}
.head-top-p1{font-size:16px; padding-left:30px;}
.head-top-p1 li{cursor:pointer; padding:0 6%; border-left:1px solid #75B2F3; height:48px; display:inline-block;}
.head-top-p1 li span{display:inline-block;}
.head-top-p1 li .cur{background:url(../images/search-cur.png) no-repeat center 48px; background-size:11px 7px; height:55px;}

.txt_wrap-1{background:#fff; border-radius:2em; margin:0 30px 20px; position:relative;}
.search_txt-1{position:relative; border:0; height:28px; line-height:28px; width:100%; border-radius:2em;}
.ly-input-wrap{padding-left:32px; border:1px solid #4D8DD1; border-radius:2em;}
.search_txt-span1{width:18px; height:18px; position:absolute; top:5px; right:10px; cursor:pointer;}

.back{width:16px; height:22px; display:inline-block; margin-top:12px; background:url(../images/arrow-l.png) no-repeat; background-size:16px 22px;}
.ly-article-1{overflow:hidden;}
.ly-article-1 ul{width:100%;}
.ly-article-1 li{height:44px; line-height:44px; border-bottom:1px solid #DEDEDE; padding-left:55px; background:url(../images/history.png) no-repeat 30px center #fff; font-size:16px; background-size:18px 18px;}
.ly-article-1 li a{display:inline-block; width:100%; color:#333;}
.ly-btn-wrap{margin:0 30px;}
.ly-btn{height:44px; line-height:44px; border:1px solid #DEDEDE; background:#fff; margin-top:20px; width:100%; font-size:16px; border-radius:8px; width:100%; cursor:pointer;}

/* 搜索页的弹出框 */
.ly-alert-1{width:100px; height:100%; background:#ccc; opacity:0.8; filter:alpha(opacity=80); display:none; margin:0 auto;}
.ly-alert-1-wrap{width:100px; border:1px solid #000; box-shadow:0 2px 0 #333; overflow:hidden;}
.ly-alert-1-wrap li{height:40px; border:1px solid #000;}
.ly-alert-1-wrap li:last{border:1px solid #000;}

/* 案例页面 */
.head-top h2{display:block; margin:0 auto; text-align:center; font-size:18px;}
.ly-like{width:22px; height:21px; display:inline-block; margin-top:-44px; cursor:pointer;}
.ly-article-1-wrap{padding:0 30px; background:#fff;}
.ly-article-1-wrap dl{border-bottom:1px solid #E5E5E5; font-size:16px; overflow:hidden; line-height:24px; padding:8px 0;}
.ly-article-1-wrap dl dt{color:#808080; display:inline-block; width:30%; float:left;}
.ly-article-1-wrap dl dd{display:inline-block; width:70%; float:left;}
.ly-btn-read{background:url(../images/read.png) no-repeat 36% center #fff; background-size:22px 21px; color:#4D8DD1; padding-left:10%;}
.content{width:100%; overflow:hidden; background:#fff; margin-top:20px; border-top:1px solid #E5E5E5; border-bottom:1px solid #E5E5E5; font-size:14px; margin-bottom:70px;}
.content-wrap{padding:10px 30px;}
.content-wrap h3{font-size:16px; color:#808080;}
.content-wrap p{ text-indent:2em; overflow:hidden; line-height:24px;}

/* detail页面 */
.ly-article-1-wrap h3{font-size:18px; padding:5px 0; border-bottom:1px solid #DEDEDE; overflow:hidden;}
.ly-alert-1-wrap-p1{background:#fff; border-bottom:1px solid #DEDEDE; overflow:hidden; font-size:16px; padding:5px 30px; color:#7F7F7F;}
.ly-alert-1-wrap-p1-span1{cursor:pointer;}
.ly-det-content{padding:10px 30px; text-indent:2em; margin-bottom:60px;}
.ly-article-1-wrap-block{display:none;}

/* list页 */
.all{width:60px; height:30px; border:1px solid #DBDBDB; background:#F5F5F5; border-radius:2em; display:inline-block; margin-top:-40px; cursor:pointer;}
.all a{color:#333;}
.ly-content{background:#fff; overflow:hidden; margin-bottom:60px;}
.ly-content-dl1{border-bottom:1px solid #E5E5E5; height:78px; padding:0 30px;}
.ly-content-dl1 dt{display:inline-block; width:20%;}
.ly-content-dl1 dt p{display:inline-block; width:30px; height:30px; line-height:30px; text-align:center; border-radius:8px; margin:24px 12px 0 0;}
.ly-content-dl1-dt1{background:#5561E7; border:1px solid #8992F6;}
.ly-content-dl1-dt2{background:#3397F1; border:1px solid #81BFF6;}
.ly-content-dl1-dt3{background:#3FD9D1; border:1px solid #9BEFEB;}
.ly-content-dl1-dt4{background:#BBF16F; border:1px solid #D9F7AF;}
.ly-content-dl1 dd{display:inline-block; width:80%;}
.ly-content-dl1 dd p{line-height:24px; width:100%; overflow:hidden; white-space:nowrap; text-overflow:ellipsis;}


/* mine页 */
.mine-top{width:100%; height:210px; background:#4D8DD1; color:#fff;}
.mine-top-wrap{padding:0 30px; position:relative; text-align:center;}
.mine-top-wrap-p1{display:inline-block; width:100px; height:30px; background:#f5f5f5; border-radius:2em; position:absolute; right:30px; top:8px;}
.mine-top-wrap-p1 a{color:#333;}
.mine-top-wrap-div1{padding:19px 0;}
.ly-mylike-wrap{width:100%;}
.ly-mylike-wrap li{padding:0 30px 0 65px; height:58px; line-height:58px; border-top:1px solid #E5E5E5; border-bottom:1px solid #E5E5E5;}
.ly-mylike-wrap li a{color:#333;}
.ly-mylike-wrap-li1{background:url(../images/mylike.png) no-repeat 30px center #fff; background-size:22px 20px;}
.ly-mylike-wrap-li1 img{display:inline-block; margin-top:19px;}
.ly-mylike-wrap-li2{background:url(../images/zhuxiao.png) no-repeat 30px center #fff; background-size:22px 20px;}


/* result页 */
.all-result{margin-top:9px;}
.ly-paixu{padding:0 30px; overflow:hidden; border-bottom:1px solid #E5E5E5;}
.ly-paixu-wrap{border:1px solid #4D8DD1; height:34px; line-height:34px; background:#fff; border-radius:8px; margin:10px 0 20px;}
.ly-paixu-wrap li{width:50%; float:left; text-align:center;}
.ly-paixu-wrap li a{color:#4D8DD1; display:inline-block; width:100%;}
.ly-paixu-wrap .cur{background:#4D8DD1;}
.ly-paixu-wrap .cur a{color:#fff;}
.ly-jieguo{padding:0 30px; border-bottom:1px solid #E5E5E5; height:30px; line-height:30px;}
.ly-paixu-wrap-span1{color:#4D8DD1;}


/* 启动页 */
.ly-start{position:relative;}
.ly-start-wrap{width:215px; height:153px; margin:90px auto;}
.ly-start-bg{position:fixed; bottom:0; left:0; width:100%;}


/* login页 */
.ly-login{padding:0 30px; overflow:hidden;}
.ly-login-wrap h1{width:97px; height:61px; padding:84px 0 40px; margin:0 auto;}
.ly-login-login{height:81px; border:1px solid #fff; margin:0 auto; border-radius:8px; padding:0 15px;}
.ly-login-login-wrap{width:100%;}
.ly-login-login-wrap input{height:40px; line-height:40px; display:block; background:none; border:0; width:100%; color:#fff; font-size:16px;}
.ly-login-login-wrap-ipt1{border-bottom:1px solid #fff;}
.ly-login-login .ly-user{background:url(../images/user-login.png) no-repeat left center; background-size:15px 17px; padding-left:25px;}
.ly-login-login .ly-pwd{background:url(../images/pwd.png) no-repeat left center; background-size:15px 17px; padding-left:25px;}
.ly-login-btn{display:block; width:100%; height:40px; line-height:40px; background:#fff; font-size:18px; color:#4D8DD1; text-align:center;}
.ly-login-footer{width:100%; text-align:center; font-size:16px; position:absolute; bottom:32px;}


/* 设置密码页面 */
.ly-wrap{padding:0 30px; font-size:16px;}
.ly-input-password{height:40px; box-shadow:0px 1px 3px rgba(0, 0, 0, 0.2); border-radius:8px; background:#fff; padding-left:20px;}
.ly-input-password input{height:40px; border-radius:8px; border:0; display:block; margin-top:20px; width:100%;}
.ly-login-btn{width:100%; height:40px; margin:20px auto 0; cursor:pointer; display:block; border:0; border-radius:8px; box-shadow:0px 1px 3px rgba(0, 0, 0, 0.2); background:#fff;}


/* 筛选1页面 */
.ly-article-shaixuan{overflow:hidden;}
.ly-article-shaixuan1-wrap{width:100%; overflow:hidden; background:#fff;}
.ly-article-shaixuan1-wrap li{padding:0 30px; height:40px; line-height:40px; border-bottom:1px solid #E5E5E5;}
.ly-article-shaixuan1-wrap li a{display:inline-block; width:100%; color:#333; background:url(../images/arrow-r.png) no-repeat right center; background-size:20px 20px;}
.ly-shaixuan{background:url(../images/queding.png) no-repeat 36% center #fff; background-size:17px 17px; color:#4D8DD1; padding-left:10%;}
.ly-shaixuan a{color:#4D8DD1;}








